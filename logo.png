            </span><span><span class="hljs-keyword">if</span></span><span> yoy_fig </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
