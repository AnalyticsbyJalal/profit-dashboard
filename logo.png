    })
    result = pd.concat([ms[[</span><span><span class="hljs-string">"Month"</span></span><span>, </span><span><span class="hljs-string">"Revenue"</span></span><span>, </span><span><span class="hljs-string">"Forecast"</span></span><span>]], future_df], ignore_index=</span><span><span class="hljs-literal">True</span></span><span>)
    result[</span><span><span class="hljs-string">"Month_str"</span></span><span>] = result[</span><span><span class="hljs-string">"Month"</span></span><span>].dt.strftime(</span><span><span class="hljs-string">"%Y-%m"</span></span><span>)
    </span><span><span class="hljs-keyword">return</span></span><span> result


