        c.drawString(col_x[</span><span><span class="hljs-number">0</span></span><span>], y, </span><span><span class="hljs-built_in">str</span></span><span>(row[</span><span><span class="hljs-string">"Product"</span></span><span>]))
        c.drawRightString(col_x[</span><span><span class="hljs-number">1</span></span><span>] + </span><span><span class="hljs-number">80</span></span><span>, y, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{row[<span class="hljs-string">'Revenue'</span></span></span></span><span>]:,</span><span><span class="hljs-number">.0</span></span><span>f}")
