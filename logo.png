</span><span><span class="hljs-comment"># Main App</span></span><span>
</span><span><span class="hljs-comment"># ----------------------------------</span></span><span>

st.title(</span><span><span class="hljs-string">" Business Profit Dashboard"</span></span><span>)
st.caption(</span><span><span class="hljs-string">"Professional corporate-style dashboard for revenue, cost, and profitability insights."</span></span><span>)

uploaded_file = st.file_uploader(</span><span><span class="hljs-string">"Upload your CSV or Excel file"</span></span><span>, </span><span><span class="hljs-built_in">type</span></span><span>=[</span><span><span class="hljs-string">"csv"</span></span><span>, </span><span><span class="hljs-string">"xlsx"</span></span><span>])

sheet_name = </span><span><span class="hljs-literal">None</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> uploaded_file </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span> </span><span><span class="hljs-keyword">and</span></span><span> uploaded_file.name.endswith(</span><span><span class="hljs-string">".xlsx"</span></span><span>):
    excel_obj = pd.ExcelFile(uploaded_file)
    sheet_name = st.selectbox(</span><span><span class="hljs-string">"Choose a sheet"</span></span><span>, excel_obj.sheet_names)

</span><span><span class="hljs-keyword">if</span></span><span> uploaded_file </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
    </span><span><span class="hljs-comment"># Load data</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> uploaded_file.name.endswith(</span><span><span class="hljs-string">".csv"</span></span><span>):
        df = pd.read_csv(uploaded_file)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        df = pd.read_excel(uploaded_file, sheet_name=sheet_name)

    st.markdown(</span><span><span class="hljs-string">"###  Full Data Preview"</span></span><span>)
    st.dataframe(df, use_container_width=</span><span><span class="hljs-literal">True</span></span><span>, height=</span><span><span class="hljs-number">350</span></span><span>)

    cols = df.columns.tolist()

    st.markdown(</span><span><span class="hljs-string">"---"</span></span><span>)
    st.markdown(</span><span><span class="hljs-string">"###  Column Mapping"</span></span><span>)

    c1, c2, c3, c4 = st.columns(</span><span><span class="hljs-number">4</span></span><span>)
    </span><span><span class="hljs-keyword">with</span></span><span> c1:
        revenue_col = st.selectbox(</span><span><span class="hljs-string">"Revenue"</span></span><span>, cols)
    </span><span><span class="hljs-keyword">with</span></span><span> c2:
        cost_col = st.selectbox(</span><span><span class="hljs-string">"Cost"</span></span><span>, [</span><span><span class="hljs-string">"None"</span></span><span>] + cols)
    </span><span><span class="hljs-keyword">with</span></span><span> c3:
        date_col = st.selectbox(</span><span><span class="hljs-string">"Date"</span></span><span>, [</span><span><span class="hljs-string">"None"</span></span><span>] + cols)
    </span><span><span class="hljs-keyword">with</span></span><span> c4:
        product_col = st.selectbox(</span><span><span class="hljs-string">"Product"</span></span><span>, [</span><span><span class="hljs-string">"None"</span></span><span>] + cols)

    </span><span><span class="hljs-comment"># Clean data</span></span><span>
    df[</span><span><span class="hljs-string">"__revenue__"</span></span><span>] = clean_number(df[revenue_col])
    df[</span><span><span class="hljs-string">"__cost__"</span></span><span>] = clean_number(df[cost_col]) </span><span><span class="hljs-keyword">if</span></span><span> cost_col != </span><span><span class="hljs-string">"None"</span></span><span> </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0</span></span><span>
    df[</span><span><span class="hljs-string">"__profit__"</span></span><span>] = df[</span><span><span class="hljs-string">"__revenue__"</span></span><span>] - df[</span><span><span class="hljs-string">"__cost__"</span></span><span>]
    df[</span><span><span class="hljs-string">"__product__"</span></span><span>] = df[product_col] </span><span><span class="hljs-keyword">if</span></span><span> product_col != </span><span><span class="hljs-string">"None"</span></span><span> </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-string">"Unknown"</span></span><span>

    </span><span><span class="hljs-keyword">if</span></span><span> date_col != </span><span><span class="hljs-string">"None"</span></span><span>:
        df[</span><span><span class="hljs-string">"__date__"</span></span><span>] = pd.to_datetime(df[date_col], errors=</span><span><span class="hljs-string">"coerce"</span></span><span>)

    </span><span><span class="hljs-comment"># ----------------------------------</span></span><span>
    </span><span><span class="hljs-comment"># KPIs</span></span><span>
    </span><span><span class="hljs-comment"># ----------------------------------</span></span><span>
    st.markdown(</span><span><span class="hljs-string">"---"</span></span><span>)
    st.markdown(</span><span><span class="hljs-string">"###  Key Metrics"</span></span><span>)

    total_revenue = df[</span><span><span class="hljs-string">"__revenue__"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    total_cost = df[</span><span><span class="hljs-string">"__cost__"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    total_profit = df[</span><span><span class="hljs-string">"__profit__"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    margin_pct = (total_profit / total_revenue * </span><span><span class="hljs-number">100</span></span><span>) </span><span><span class="hljs-keyword">if</span></span><span> total_revenue </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0</span></span><span>

    k1, k2, k3, k4 = st.columns(</span><span><span class="hljs-number">4</span></span><span>)
    k1.metric(</span><span><span class="hljs-string">"Total Revenue"</span></span><span>, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{total_revenue:,<span class="hljs-number">.0</span></span></span></span><span>f}")
    k2.metric(</span><span><span class="hljs-string">"Total Cost"</span></span><span>, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{total_cost:,<span class="hljs-number">.0</span></span></span></span><span>f}")
    k3.metric(</span><span><span class="hljs-string">"Total Profit"</span></span><span>, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{total_profit:,<span class="hljs-number">.0</span></span></span></span><span>f}")
    k4.metric(</span><span><span class="hljs-string">"Profit Margin"</span></span><span>, </span><span><span class="hljs-string">f"<span class="hljs-subst">{margin_pct:<span class="hljs-number">.1</span></span></span></span><span>f}%")

    </span><span><span class="hljs-comment"># ----------------------------------</span></span><span>
    </span><span><span class="hljs-comment"># Charts</span></span><span>
    </span><span><span class="hljs-comment"># ----------------------------------</span></span><span>
    st.markdown(</span><span><span class="hljs-string">"---"</span></span><span>)
    st.markdown(</span><span><span class="hljs-string">"###  Performance Charts"</span></span><span>)

    left, right = st.columns(</span><span><span class="hljs-number">2</span></span><span>)

    </span><span><span class="hljs-comment"># Monthly chart</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> date_col != </span><span><span class="hljs-string">"None"</span></span><span>:
        df2 = df.dropna(subset=[</span><span><span class="hljs-string">"__date__"</span></span><span>]).copy()
        df2[</span><span><span class="hljs-string">"month"</span></span><span>] = df2[</span><span><span class="hljs-string">"__date__"</span></span><span>].dt.to_period(</span><span><span class="hljs-string">"M"</span></span><span>).dt.to_timestamp()
        monthly = df2.groupby(</span><span><span class="hljs-string">"month"</span></span><span>)[[</span><span><span class="hljs-string">"__revenue__"</span></span><span>, </span><span><span class="hljs-string">"__profit__"</span></span><span>]].</span><span><span class="hljs-built_in">sum</span></span><span>().reset_index()
        monthly.columns = [</span><span><span class="hljs-string">"Month"</span></span><span>, </span><span><span class="hljs-string">"Revenue"</span></span><span>, </span><span><span class="hljs-string">"Profit"</span></span><span>]
        monthly[</span><span><span class="hljs-string">"Month_str"</span></span><span>] = monthly[</span><span><span class="hljs-string">"Month"</span></span><span>].dt.strftime(</span><span><span class="hljs-string">"%Y-%m"</span></span><span>)

        </span><span><span class="hljs-keyword">with</span></span><span> left:
            fig = px.line(monthly, x=</span><span><span class="hljs-string">"Month_str"</span></span><span>, y=[</span><span><span class="hljs-string">"Revenue"</span></span><span>, </span><span><span class="hljs-string">"Profit"</span></span><span>], title=</span><span><span class="hljs-string">"Revenue &amp; Profit Trend"</span></span><span>)
            fig = style_fig(fig)
            st.plotly_chart(fig, use_container_width=</span><span><span class="hljs-literal">True</span></span><span>)

    </span><span><span class="hljs-comment"># Top products chart</span></span><span>
    product_summary = build_product_summary(df)

    </span><span><span class="hljs-keyword">with</span></span><span> right:
        fig2 = px.bar(product_summary.head(</span><span><span class="hljs-number">10</span></span><span>), x=</span><span><span class="hljs-string">"Product"</span></span><span>, y=</span><span><span class="hljs-string">"Profit"</span></span><span>, title=</span><span><span class="hljs-string">"Top 10 Products by Profit"</span></span><span>)
        fig2 = style_fig(fig2)
        st.plotly_chart(fig2, use_container_width=</span><span><span class="hljs-literal">True</span></span><span>)

    </span><span><span class="hljs-comment"># ----------------------------------</span></span><span>
    </span><span><span class="hljs-comment"># Product Table</span></span><span>
    </span><span><span class="hljs-comment"># ----------------------------------</span></span><span>
    st.markdown(</span><span><span class="hljs-string">"###  Product Profit Summary"</span></span><span>)
    st.dataframe(product_summary, use_container_width=</span><span><span class="hljs-literal">True</span></span><span>)

</span><span><span class="hljs-keyword">else</span></span><span>:
    st.info(</span><span><span class="hljs-string">" Upload a file to begin."</span></span><span>)
