    ms[</span><span><span class="hljs-string">"Forecast"</span></span><span>] = intercept + slope * x
    </span><span><span class="hljs-comment"># future periods</span></span><span>
    last_month = ms[</span><span><span class="hljs-string">"Month"</span></span><span>].iloc[-</span><span><span class="hljs-number">1</span></span><span>]
    future_months = [last_month + pd.offsets.MonthBegin(i + </span><span><span class="hljs-number">1</span></span><span>) </span><span><span class="hljs-keyword">for</span></span><span> i </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(periods)]
    x_future = np.arange(</span><span><span class="hljs-built_in">len</span></span><span>(ms), </span><span><span class="hljs-built_in">len</span></span><span>(ms) + periods)
    future_values = intercept + slope * x_future
    future_df = pd.DataFrame({
