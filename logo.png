        df[</span><span><span class="hljs-string">"unit_price"</span></span><span>] = </span><span><span class="hljs-number">0.0</span></span><span>

    </span><span><span class="hljs-keyword">if</span></span><span> cost_col:
        df[</span><span><span class="hljs-string">"unit_cost"</span></span><span>] = df[cost_col].apply(clean_number)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        df[</span><span><span class="hljs-string">"unit_cost"</span></span><span>] = </span><span><span class="hljs-number">0.0</span></span><span>

    </span><span><span class="hljs-keyword">if</span></span><span> total_col:
        df[</span><span><span class="hljs-string">"order_total"</span></span><span>] = df[total_col].apply(clean_number)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        </span><span><span class="hljs-comment"># fallback: quantity * price</span></span><span>
        df[</span><span><span class="hljs-string">"order_total"</span></span><span>] = df[</span><span><span class="hljs-string">"qty"</span></span><span>] * df[</span><span><span class="hljs-string">"unit_price"</span></span><span>]

    </span><span><span class="hljs-comment"># Revenue &amp; cost at line level</span></span><span>
    df[</span><span><span class="hljs-string">"revenue"</span></span><span>] = df[</span><span><span class="hljs-string">"qty"</span></span><span>] * df[</span><span><span class="hljs-string">"unit_price"</span></span><span>]
    df[</span><span><span class="hljs-string">"cost"</span></span><span>] = df[</span><span><span class="hljs-string">"qty"</span></span><span>] * df[</span><span><span class="hljs-string">"unit_cost"</span></span><span>]
    df[</span><span><span class="hljs-string">"gross_profit"</span></span><span>] = df[</span><span><span class="hljs-string">"revenue"</span></span><span>] - df[</span><span><span class="hljs-string">"cost"</span></span><span>]

    </span><span><span class="hljs-comment"># Overall KPIs</span></span><span>
    total_revenue = df[</span><span><span class="hljs-string">"revenue"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    total_cost = df[</span><span><span class="hljs-string">"cost"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    gross_profit = df[</span><span><span class="hljs-string">"gross_profit"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    gross_margin = (gross_profit / total_revenue * </span><span><span class="hljs-number">100</span></span><span>) </span><span><span class="hljs-keyword">if</span></span><span> total_revenue </span><span><span class="hljs-keyword">else</span></span><span> 
