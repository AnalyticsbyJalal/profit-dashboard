        plot_bgcolor=</span><span><span class="hljs-string">"#ffffff"</span></span><span>,
        paper_bgcolor=</span><span><span class="hljs-string">"#ffffff"</span></span><span>,
        margin=</span><span><span class="hljs-built_in">dict</span></span><span>(l=</span><span><span class="hljs-number">40</span></span><span>, r=</span><span><span class="hljs-number">20</span></span><span>, t=</span><span><span class="hljs-number">60</span></span><span>, b=</span><span><span class="hljs-number">40</span></span><span>),
        colorway=[</span><span><span class="hljs-string">"#2b6cb0"</span></span><span>, </span><span><span class="hljs-string">"#2f855a"</span></span><span>, </span><span><span class="hljs-string">"#718096"</span></span><span>]
    )
    fig.update_xaxes(showgrid=</span><span><span class="hljs-literal">True</span></span><span>, gridcolor=</span><span><span class="hljs-string">"#e2e8f0"</span></span><span>)
    fig.update_yaxes(showgrid=</span><span><span class="hljs-literal">True</span></span><span>, gridcolor=</span><span><span class="hljs-string">"#e2e8f0"</span></span><span>)
    </span><span><span class="hljs-keyword">return</span></span><span> fig


