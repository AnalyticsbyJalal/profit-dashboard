    headers = [</span><span><span class="hljs-string">"Product"</span></span><span>, </span><span><span class="hljs-string">"Revenue"</span></span><span>, </span><span><span class="hljs-string">"Cost"</span></span><span>, </span><span><span class="hljs-string">"Profit"</span></span><span>]
    col_x = [</span><span><span class="hljs-number">40</span></span><span>, </span><span><span class="hljs-number">200</span></span><span>, </span><span><span class="hljs-number">320</span></span><span>, </span><span><span class="hljs-number">440</span></span><span>]
    </span><span><span class="hljs-keyword">for</span></span><span> x, h </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">zip</span></span><span>(col_x, headers):
