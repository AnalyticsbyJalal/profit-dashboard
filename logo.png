    </span><span><span class="hljs-keyword">if</span></span><span> line_fig </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
        line_png = line_fig.to_image(</span><span><span class="hljs-built_in">format</span></span><span>=</span><span><span class="hljs-string">"png"</span></span><span>)
        line_img = ImageReader(BytesIO(line_png))
        c.drawImage(line_img, </span><span><span class="hljs-number">40</span></span><span>, y - </span><span><span class="hljs-number">210</span></span><span>, width=</span><span><span class="hljs-number">520</span></span><span>, height=</span><span><span class="hljs-number">200</span></span><span>, preserveAspectRatio=</span><span><span class="hljs-literal">True</span></span><span>)
        y -= </span><span><span class="hljs-number">230</span></span><span>

    </span><span><span class="hljs-keyword">if</span></span><span> bar_fig </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
        bar_png = bar_fig.to_image(</span><span><span class="hljs-built_in">format</span></span><span>=</span><span><span class="hljs-string">"png"</span></span><span>)
        bar_img = ImageReader(BytesIO(bar_png))
        c.drawImage(bar_img, </span><span><span class="hljs-number">40</span></span><span>, y - </span><span><span class="hljs-number">210</span></span><span>, width=</span><span><span class="hljs-number">520</span></span><span>, height=</span><span><span class="hljs-number">200</span></span><span>, preserveAspectRatio=</span><span><span class="hljs-literal">True</span></span><span>)
        y -= </span><span><span class="hljs-number">230</span></span><span>

    </span><span><span class="hljs-comment"># New page for table if needed</span></span><span>
    c.showPage()
    c.setFont(</span><span><span class="hljs-string">"Helvetica-Bold"</span></span><span>, </span><span><span class="hljs-number">14</span></span><span>)
    c.drawString(</span><span><span class="hljs-number">40</span></span><span>, height - </span><span><span class="hljs-number">50</span></span><span>, </span><span><span class="hljs-string">"Product Profit Summary"</span></span><span>)
    c.setFont(</span><span><span class="hljs-string">"Helvetica"</span></span><span>, </span><span><span class="hljs-number">9</span></span><span>)

    </span><span><span class="hljs-comment"># Table header</span></span><span>
