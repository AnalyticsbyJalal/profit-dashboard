        c.drawRightString(col_x[</span><span><span class="hljs-number">3</span></span><span>] + </span><span><span class="hljs-number">80</span></span><span>, y, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{row[<span class="hljs-string">'Profit'</span></span></span></span><span>]:,</span><span><span class="hljs-number">.0</span></span><span>f}")
        y -= </span><span><span class="hljs-number">14</span></span><span>

    c.save()
