6.01057 5.53708 6.00551 6.00391 6.00391C6.00551 5.53708 6.01057 5.14051 6.03809 4.80371C6.0755 4.34588 6.15483 3.94012 6.34668 3.56348L6.46875 3.34473C6.77282 2.84912 7.20856 2.44514 7.72949 2.17969L7.87207 2.11328C8.20855 1.96886 8.56979 1.90385 8.9707 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-keyword">import</span></span><span> streamlit </span><span><span class="hljs-keyword">as</span></span><span> st
</span><span><span class="hljs-keyword">import</span></span><span> pandas </span><span><span class="hljs-keyword">as</span></span><span> pd
</span><span><span class="hljs-keyword">import</span></span><span> plotly.express </span><span><span class="hljs-keyword">as</span></span><span> px

st.set_page_config(page_title=</span><span><span class="hljs-string">"Business Profit Dashboard"</span></span><span>, layout=</span><span><span class="hljs-string">"wide"</span></span><span>)


</span><span><span class="hljs-comment"># ---------- Helpers ----------</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">clean_number</span></span><span>(</span><span><span class="hljs-params">series: pd.Series</span></span><span>) -&gt; pd.Series:
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">_clean</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
        </span><span><span class="hljs-keyword">if</span></span><span> pd.isna(x):
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(x, (</span><span><span class="hljs-built_in">int</span></span><span>, </span><span><span class="hljs-built_in">float</span></span><span>)):
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">float</span></span><span>(x)
        x = </span><span><span class="hljs-built_in">str</span></span><span>(x)
        x = x.replace(</span><span><span class="hljs-string">"$"</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>).replace(</span><span><span class="hljs-string">","</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>).strip()
        </span><span><span class="hljs-keyword">try</span></span><span>:
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">float</span></span><span>(x)
        </span><span><span class="hljs-keyword">except</span></span><span> ValueError:
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>

    </span><span><span class="hljs-keyword">return</span></span><span> series.apply(_clean)


</span><span><span class="hljs-comment"># ---------- UI ----------</span></span><span>
st.title(</span><span><span class="hljs-string">" Business Profit Dashboard"</span></span><span>)

st.write(
    <span class="hljs-string">"""
Upload a CSV with your sales data and get instant revenue, cost, profit and margin insights.  
You can use this with Shopify exports, Amazon reports, or any custom sales file.
"""</span>
)

uploaded_file = st.file_uploader(</span><span><span class="hljs-string">"Upload your CSV file"</span></span><span>, </span><span><span class="hljs-built_in">type</span></span><span>=[</span><span><span class="hljs-string">"csv"</span></span><span>])

</span><span><span class="hljs-keyword">if</span></span><span> uploaded_file </span><span><span class="hljs-keyword">is</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>:
    df = pd.read_csv(uploaded_file)

    st.subheader(</span><span><span class="hljs-string">" Data preview"</span></span><span>)
    st.dataframe(df.head())

    cols = df.columns.tolist()

    st.markdown(</span><span><span class="hljs-string">"###  Map your columns"</span></span><span>)
    col1, col2, col3, col4 = st.columns(</span><span><span class="hljs-number">4</span></span><span>)

    </span><span><span class="hljs-keyword">with</span></span><span> col1:
        revenue_col = st.selectbox(</span><span><span class="hljs-string">"Revenue column"</span></span><span>, cols)

    </span><span><span class="hljs-keyword">with</span></span><span> col2:
        cost_col = st.selectbox(</span><span><span class="hljs-string">"Cost column (optional)"</span></span><span>, [</span><span><span class="hljs-string">"None"</span></span><span>] + cols)

    </span><span><span class="hljs-keyword">with</span></span><span> col3:
        date_col = st.selectbox(</span><span><span class="hljs-string">"Date column (optional)"</span></span><span>, [</span><span><span class="hljs-string">"None"</span></span><span>] + cols)

    </span><span><span class="hljs-keyword">with</span></span><span> col4:
        product_col = st.selectbox(</span><span><span class="hljs-string">"Product column (optional)"</span></span><span>, [</span><span><span class="hljs-string">"None"</span></span><span>] + cols)

    </span><span><span class="hljs-comment"># Clean numeric columns</span></span><span>
    df[</span><span><span class="hljs-string">"__revenue__"</span></span><span>] = clean_number(df[revenue_col])

    </span><span><span class="hljs-keyword">if</span></span><span> cost_col != </span><span><span class="hljs-string">"None"</span></span><span>:
        df[</span><span><span class="hljs-string">"__cost__"</span></span><span>] = clean_number(df[cost_col])
    </span><span><span class="hljs-keyword">else</span></span><span>:
        df[</span><span><span class="hljs-string">"__cost__"</span></span><span>] = </span><span><span class="hljs-number">0.0</span></span><span>

    df[</span><span><span class="hljs-string">"__profit__"</span></span><span>] = df[</span><span><span class="hljs-string">"__revenue__"</span></span><span>] - df[</span><span><span class="hljs-string">"__cost__"</span></span><span>]

    </span><span><span class="hljs-comment"># Date handling</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> date_col != </span><span><span class="hljs-string">"None"</span></span><span>:
        df[</span><span><span class="hljs-string">"__date__"</span></span><span>] = pd.to_datetime(df[date_col], errors=</span><span><span class="hljs-string">"coerce"</span></span><span>)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        df[</span><span><span class="hljs-string">"__date__"</span></span><span>] = pd.NaT

    </span><span><span class="hljs-comment"># Product handling</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> product_col != </span><span><span class="hljs-string">"None"</span></span><span>:
        df[</span><span><span class="hljs-string">"__product__"</span></span><span>] = df[product_col].astype(</span><span><span class="hljs-built_in">str</span></span><span>)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        df[</span><span><span class="hljs-string">"__product__"</span></span><span>] = </span><span><span class="hljs-string">"Unknown"</span></span><span>


    </span><span><span class="hljs-comment"># ---------- KPIs ----------</span></span><span>
    total_revenue = df[</span><span><span class="hljs-string">"__revenue__"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    total_cost = df[</span><span><span class="hljs-string">"__cost__"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    total_profit = df[</span><span><span class="hljs-string">"__profit__"</span></span><span>].</span><span><span class="hljs-built_in">sum</span></span><span>()
    margin_pct = (total_profit / total_revenue * </span><span><span class="hljs-number">100</span></span><span>) </span><span><span class="hljs-keyword">if</span></span><span> total_revenue </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>

    st.markdown(</span><span><span class="hljs-string">"###  Key Metrics"</span></span><span>)
    kpi1, kpi2, kpi3, kpi4 = st.columns(</span><span><span class="hljs-number">4</span></span><span>)

    kpi1.metric(</span><span><span class="hljs-string">"Total Revenue"</span></span><span>, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{total_revenue:,<span class="hljs-number">.0</span></span></span></span><span>f}")
    kpi2.metric(</span><span><span class="hljs-string">"Total Cost"</span></span><span>, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{total_cost:,<span class="hljs-number">.0</span></span></span></span><span>f}")
    kpi3.metric(</span><span><span class="hljs-string">"Total Profit"</span></span><span>, </span><span><span class="hljs-string">f"$<span class="hljs-subst">{total_profit:,<span class="hljs-number">.0</span></span></span></span><span>f}")
    kpi4.metric(</span><span><span class="hljs-string">"Profit Margin"</span></span><span>, </span><span><span class="hljs-string">f"<span class="hljs-subst">{margin_pct:,<span class="hljs-number">.1</span></span></span></span><span>f}%")

    </span><span><span class="hljs-comment"># ---------- Trends ----------</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> df[</span><span><span class="hljs-string">"__date__"</span></span><span>].notna().</span><span><span class="hljs-built_in">any</span></span><span>():
        st.markdown(</span><span><span class="hljs-string">"###  Revenue &amp; Profit Over Time"</span></span><span>)

        </span><span><span class="hljs-comment"># Group by month</span></span><span>
        temp = df.dropna(subset=[</span><span><span class="hljs-string">"__date__"</span></span><span>]).copy()
        temp[</span><span><span class="hljs-string">"month"</span></span><span>] = temp[</span><span><span class="hljs-string">"__date__"</span></span><span>].dt.to_period(</span><span><span class="hljs-string">"M"</span></span><span>).dt.to_timestamp()

        monthly = (
            temp.groupby(</span><span><span class="hljs-string">"month"</span></span><span>)[[</span><span><span class="hljs-string">"__revenue__"</span></span><span>, </span><span><span class="hljs-string">"__cost__"</span></span><span>, </span><span><span class="hljs-string">"__profit__"</span></span><span>]]
            .</span><span><span class="hljs-built_in">sum</span></span><span>()
            .reset_index()
        )

        line_fig = px.line(
            monthly,
            x=</span><span><span class="hljs-string">"month"</span></span><span>,
            y=[</span><span><span class="hljs-string">"__revenue__"</span></span><span>, </span><span><span class="hljs-string">"__profit__"</span></span><span>],
            labels={
                </span><span><span class="hljs-string">"value"</span></span><span>: </span><span><span class="hljs-string">"Amount"</span></span><span>,
                </span><span><span class="hljs-string">"month"</span></span><span>: </span><span><span class="hljs-string">"Month"</span></span><span>,
                </span><span><span class="hljs-string">"__revenue__"</span></span><span>: </span><span><span class="hljs-string">"Revenue"</span></span><span>,
                </span><span><span class="hljs-string">"__profit__"</span></span><span>: </span><span><span class="hljs-string">"Profit"</span></span><span>,
            },
            title=</span><span><span class="hljs-string">"Monthly Revenue &amp; Profit"</span></span><span>,
        )
        st.plotly_chart(line_fig, use_container_width=</span><span><span class="hljs-literal">True</span></span><span>)

    </span><span><span class="hljs-comment"># ---------- Top products ----------</span></span><span>
    st.markdown(</span><span><span class="hljs-string">"###  Top Products by Profit"</span></span><span>)

    product_summary = (
        df.groupby(</span><span><span class="hljs-string">"__product__"</span></span><span>)[[</span><span><span class="hljs-string">"__revenue__"</span></span><span>, </span><span><span class="hljs-string">"__profit__"</span></span><span>]]
        .</span><span><span class="hljs-built_in">sum</span></span><span>()
        .sort_values(</span><span><span class="hljs-string">"__profit__"</span></span><span>, ascending=</span><span><span class="hljs-literal">False</span></span><span>)
        .reset_index()
    )

    product_summary.rename(
        columns={</span><span><span class="hljs-string">"__product__"</span></span><span>: </span><span><span class="hljs-string">"Product"</span></span><span>, </span><span><span class="hljs-string">"__revenue__"</span></span><span>: </span><span><span class="hljs-string">"Revenue"</span></span><span>, </span><span><span class="hljs-string">"__profit__"</span></span><span>: </span><span><span class="hljs-string">"Profit"</span></span><span>},
        inplace=</span><span><span class="hljs-literal">True</span></span><span>,
    )

    st.dataframe(product_summary.head(</span><span><span class="hljs-number">20</span></span><span>))

    bar_fig = px.bar(
        product_summary.head(</span><span><span class="hljs-number">10</span></span><span>),
        x=</span><span><span class="hljs-string">"Product"</span></span><span>,
        y=</span><span><span class="hljs-string">"Profit"</span></span><span>,
        title=</span><span><span class="hljs-string">"Top 10 Products by Profit"</span></span><span>,
    )
    st.plotly_chart(bar_fig, use_container_width=</span><span><span class="hljs-literal">True</span></span><span>)

</span><span><span class="hljs-keyword">else</span></span><span>:
    st.info(</span><span><span class="hljs-string">" Upload a CSV to get started."</span></span><span>)
</span></span></code></div></div></pre>
