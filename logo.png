</span><span><span class="hljs-comment"># ---------- Helpers ----------</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">clean_number</span></span><span>(</span><span><span class="hljs-params">series: pd.Series</span></span><span>) -&gt; pd.Series:
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">_clean</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
        </span><span><span class="hljs-keyword">if</span></span><span> pd.isna(x):
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(x, (</span><span><span class="hljs-built_in">int</span></span><span>, </span><span><span class="hljs-built_in">float</span></span><span>)):
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">float</span></span><span>(x)
        x = </span><span><span class="hljs-built_in">str</span></span><span>(x)
        x = x.replace(</span><span><span class="hljs-string">"$"</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>).replace(</span><span><span class="hljs-string">","</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>).strip()
        </span><span><span class="hljs-keyword">try</span></span><span>:
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">float</span></span><span>(x)
        </span><span><span class="hljs-keyword">except</span></span><span> ValueError:
            </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>

    </span><span><span class="hljs-keyword">return</span></span><span> series.apply(_clean)


</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">build_summary</span></span><span>(</span><span><span class="hljs-params">df: pd.DataFrame</span></span><span>) -&gt; pd.DataFrame:
    </span><span><span class="hljs-string">"""Summary by product."""</span></span><span>
    summary = (
        df.groupby(</span><span><span class="hljs-string">"__product__"</span></span><span>)[[</span><span><span class="hljs-string">"__revenue__"</span></span><span>, </span><span><span class="hljs-string">"__cost__"</span></span><span>, </span><span><span class="hljs-string">"__profit__"</span></span><span>]]
        .</span><span><span class="hljs-built_in">sum</span></span><span>()
        .sort_values(</span><span><span class="hljs-string">"__profit__"</span></span><span>, ascending=</span><span><span class="hljs-literal">False</span></span><span>)
        .reset_index()
    )
    summary.rename(
        columns={
            </span><span><span class="hljs-string">"__product__"</span></span><span>: </span><span><span class="hljs-string">"Product"</span></span><span>,
            </span><span><span class="hljs-string">"__revenue__"</span></span><span>: </span><span><span class="hljs-string">"Revenue"</span></span><span>,
            </span><span><span class="hljs-string">"__cost__"</span></span><span>: </span><span><span class="hljs-string">"Cost"</span></span><span>,
            </span><span><span class="hljs-string">"__profit__"</span></span><span>: </span><span><span class="hljs-string">"Profit"</span></span><span>,
        },
        inplace=</span><span><span class="hljs-literal">True</span></span><span>,
    )
    </span><span><span class="hljs-keyword">return</span></span><span> summary


</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">generate_pdf_report</span></span><span>(<span class="hljs-params">
    total_revenue,
    total_cost,
    total_profit,
    margin_pct,
    line_fig,
    bar_fig,
    summary_df: pd.DataFrame,
</span>) -&gt; </span><span><span class="hljs-built_in">bytes</span></span><span>:
    </span><span><span class="hljs-string">"""Build a PDF report with KPIs, charts, and product table."""</span></span><span>
