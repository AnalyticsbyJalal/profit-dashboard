</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">clean_number</span></span><span>(</span><span><span class="hljs-params">series</span></span><span>):
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">_clean</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
        </span><span><span class="hljs-keyword">if</span></span><span> pd.isna(x): </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(x, (</span><span><span class="hljs-built_in">int</span></span><span>, </span><span><span class="hljs-built_in">float</span></span><span>)): </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">float</span></span><span>(x)
        x = </span><span><span class="hljs-built_in">str</span></span><span>(x).replace(</span><span><span class="hljs-string">"$"</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>).replace(</span><span><span class="hljs-string">","</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>).strip()
        </span><span><span class="hljs-keyword">try</span></span><span>: </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">float</span></span><span>(x)
        </span><span><span class="hljs-keyword">except</span></span><span>: </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>
    </span><span><span class="hljs-keyword">return</span></span><span> series.apply(_clean)


</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">build_product_summary</span></span><span>(</span><span><span class="hljs-params">df</span></span><span>):
    summary = (
        df.groupby(</span><span><span class="hljs-string">"__product__"</span></span><span>)[[</span><span><span class="hljs-string">"__revenue__"</span></span><span>, </span><span><span class="hljs-string">"__cost__"</span></span><span>, </span><span><span class="hljs-string">"__profit__"</span></span><span>]]
        .</span><span><span class="hljs-built_in">sum</span></span><span>()
        .sort_values(</span><span><span class="hljs-string">"__profit__"</span></span><span>, ascending=</span><span><span class="hljs-literal">False</span></span><span>)
        .reset_index()
    )
    summary.columns = [</span><span><span class="hljs-string">"Product"</span></span><span>, </span><span><span class="hljs-string">"Revenue"</span></span><span>, </span><span><span class="hljs-string">"Cost"</span></span><span>, </span><span><span class="hljs-string">"Profit"</span></span><span>]
    summary[</span><span><span class="hljs-string">"Margin %"</span></span><span>] = ((summary[</span><span><span class="hljs-string">"Profit"</span></span><span>] / summary[</span><span><span class="hljs-string">"Revenue"</span></span><span>]) * </span><span><span class="hljs-number">100</span></span><span>).</span><span><span class="hljs-built_in">round</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>)
    </span><span><span class="hljs-keyword">return</span></span><span> summary


</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">style_fig</span></span><span>(</span><span><span class="hljs-params">fig</span></span><span>):
    </span><span><span class="hljs-string">"""Corporate Blue Plotly styling."""</span></span><span>
    fig.update_layout(
        template=</span><span><span class="hljs-string">"simple_white"</span></span><span>,
        title_font_color=</span><span><span class="hljs-string">"#234a7c"</span></span><span>,
