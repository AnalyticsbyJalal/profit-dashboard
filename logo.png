    ms = monthly.sort_values(</span><span><span class="hljs-string">"Month"</span></span><span>).reset_index(drop=</span><span><span class="hljs-literal">True</span></span><span>)
    x = np.arange(</span><span><span class="hljs-built_in">len</span></span><span>(ms))
